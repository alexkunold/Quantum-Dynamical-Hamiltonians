          RowBox[{"\[Nu]t1", ".", "Mk"}], "+", 
          RowBox[{"Ik", ".", "J"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"\[Nu]t1", "=", "\[Nu]t"}], ";"}], "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"k1", ",", "2", ",", "dim"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Transpose", "[", "\[Nu]t", "]"}]}]}], "\[IndentingNewLine]", 
