Cell[BoxData[
 RowBox[{
  RowBox[{"LieGetNu", "[", 
   RowBox[{"M_", ",", "J_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"dim", ",", "Mk", ",", "Ik", ",", "\[Nu]t", ",", "\[Nu]t1"}], 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"dim", "=", 
      RowBox[{
       RowBox[{"Dimensions", "[", 
        RowBox[{"M", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Ik", "=", 
      RowBox[{"Normal", "[", 
       RowBox[{"SparseArray", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"1", ",", "1"}], "}"}], "\[Rule]", "1"}], "}"}], ",", 
         "dim"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"\[Nu]t1", "=", 
      RowBox[{"Ik", ".", "J"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"Mk", "=", 
         RowBox[{"M", "[", 
          RowBox[{"[", "k1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Ik", "=", 
         RowBox[{"Normal", "[", 
          RowBox[{"SparseArray", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"k1", ",", "k1"}], "}"}], "\[Rule]", "1"}], "}"}], ",",
             "dim"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"\[Nu]t", "=", 
         RowBox[{
          RowBox[{"\[Nu]t1", ".", "Mk"}], "+", 
          RowBox[{"Ik", ".", "J"}]}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"\[Nu]t1", "=", "\[Nu]t"}], ";"}], "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"k1", ",", "2", ",", "dim"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Transpose", "[", "\[Nu]t", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
