Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"m", "=", "100"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[ScriptM]", "=", "1.0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Omega]0", "=", "2.0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Omega]min", "=", "5.0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[Omega]max", "=", "35.0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"\[CapitalDelta]\[Omega]", "=", "3.0"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dataNM", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"dataN\[CapitalOmega]", "=", 
   RowBox[{"{", "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"prog", "=", "0.0"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"ProgressIndicator", "[", 
  RowBox[{"Dynamic", "[", "prog", "]"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{"Do", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"prog", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[Omega]", "-", "\[Omega]min"}], ")"}], "/", 
      RowBox[{"(", 
       RowBox[{"\[Omega]max", "-", "\[Omega]min"}], ")"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"T", "=", 
     RowBox[{"2", 
      RowBox[{"\[Pi]", "/", "\[Omega]"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"v\[Alpha]t", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Subscript", "[", 
         RowBox[{"\[Alpha]", ",", "k1"}], "]"}], "[", "t", "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"k1", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"difeqstn", "=", 
     RowBox[{"difeqst", "/.", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         SubscriptBox["a", "1"], "\[Rule]", 
         RowBox[{
          FractionBox["\[ScriptM]", "2"], " ", 
          SuperscriptBox["\[Omega]0", "2"], 
          RowBox[{"Cos", "[", 
           RowBox[{"\[Omega]", " ", "t"}], "]"}]}]}], ",", 
        RowBox[{
         SubscriptBox["a", "2"], "\[Rule]", "0"}], ",", 
        RowBox[{
         SubscriptBox["a", "3"], "\[Rule]", 
         FractionBox["1", 
          RowBox[{"2", "\[ScriptM]"}]]}]}], "}"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"solt", "=", 
     RowBox[{
      RowBox[{"NDSolve", "[", 
       RowBox[{"difeqstn", ",", "v\[Alpha]t", ",", 
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "T"}], "}"}]}], "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"v\[Alpha]T", "=", 
     RowBox[{
      RowBox[{"v\[Alpha]t", "/.", "solt"}], "/.", 
      RowBox[{"{", 
       RowBox[{"t", "\[Rule]", "T"}], "}"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"evaln", "=", 
     RowBox[{
      RowBox[{"eval", "[", 
       RowBox[{"[", "1", "]"}], "]"}], "/.", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"v\[Alpha]", "[", 
          RowBox[{"[", "k1", "]"}], "]"}], "\[Rule]", 
         RowBox[{"v\[Alpha]T", "[", 
          RowBox[{"[", "k1", "]"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k1", ",", "1", ",", "n"}], "}"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"evecn", "=", 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Alpha]", "3"], 
       RowBox[{"evec", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], "/.", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"v\[Alpha]", "[", 
          RowBox[{"[", "k1", "]"}], "]"}], "\[Rule]", 
         RowBox[{"v\[Alpha]T", "[", 
          RowBox[{"[", "k1", "]"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"k1", ",", "1", ",", "n"}], "}"}]}], "]"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"\[Rho]", "=", "evecn"}], ";", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"\[Chi]", "[", "\[Gamma]_", "]"}], ":=", 
     SuperscriptBox[
      RowBox[{"Norm", "[", 
       RowBox[{"v\[Alpha]T", "-", 
        RowBox[{"LieGetNAlpha", "[", 
         RowBox[{"J", ",", "\[Nu]i", ",", "v\[Alpha]", ",", 
          RowBox[{"\[Gamma]", " ", "\[Rho]"}], ",", "m"}], "]"}]}], "]"}], 
      "2"]}], ";", "\[IndentingNewLine]", 
    RowBox[{"\[Gamma]", "=", 
     RowBox[{
      RowBox[{"Sort", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"\[Gamma]", ",", 
            RowBox[{"\[Chi]", "[", "\[Gamma]", "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"\[Gamma]", ",", "0.45", ",", "1.01", ",", "0.0005"}], 
           "}"}]}], "]"}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"#1", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "<", 
          RowBox[{"#2", "[", 
           RowBox[{"[", "2", "]"}], "]"}]}], "&"}]}], "]"}], "[", 
      RowBox[{"[", 
       RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"v\[Beta]N", "=", 
     RowBox[{"\[Gamma]", " ", "\[Rho]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"AppendTo", "[", 
     RowBox[{"dataNM", ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Omega]0", "/", "\[Omega]"}], ",", 
        FractionBox["\[Pi]", 
         RowBox[{"\[ScriptM]", " ", "\[Omega]", " ", 
          RowBox[{"v\[Beta]N", "[", 
           RowBox[{"[", "3", "]"}], "]"}]}]]}], "}"}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"AppendTo", "[", 
     RowBox[{"dataN\[CapitalOmega]", ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"\[Omega]0", "/", "\[Omega]"}], ",", 
        RowBox[{
         FractionBox["1", "\[Pi]"], " ", 
         SqrtBox[
          RowBox[{
           RowBox[{"v\[Beta]N", "[", 
            RowBox[{"[", "1", "]"}], "]"}], 
           RowBox[{"v\[Beta]N", "[", 
            RowBox[{"[", "3", "]"}], "]"}]}]]}]}], "}"}]}], "]"}], ";"}], 
   "\[IndentingNewLine]", ",", 
   RowBox[{"{", 
    RowBox[{
    "\[Omega]", ",", "\[Omega]min", ",", "\[Omega]max", ",", 
     "\[CapitalDelta]\[Omega]"}], "}"}]}], "]"}]}], "Input",
